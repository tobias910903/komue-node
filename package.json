{
    "name": "komue-server",
    "version": "1.0.0",
    "description": "Nodejs + express + pm2",
    "author": "lihuyong <admin@lihuyong.com>",
    "private": true,
    "scripts": {
        "start:test": "cross-env ENV_CONFIG=test pm2 start pm2.json",
        "restart:test": "cross-env ENV_CONFIG=test pm2 restart pm2.json",
        "start": "cross-env ENV_CONFIG=prod pm2 start pm2.json",
        "restart": "cross-env ENV_CONFIG=prod pm2 restart pm2.json",
        "stop": "pm2 stop all",
        "delete": "pm2 delete all"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cross-env": "^5.2.0",
        "express-session": "^1.17.0",
        "md5-node": "^1.0.1",
        "mysql": "^2.17.1",
        "pm2": "^3.5.1",
        "redis": "^2.8.0",
        "socket.io": "^2.3.0",
        "chalk": "^2.4.2",
        "express": "^4.17.1",
        "fs": "0.0.1-security",
        "html-webpack-plugin": "^2.30.1",
        "path": "^0.12.7"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
