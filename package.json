{
    "name": "komue-node",
    "version": "1.0.0",
    "description": "Nodejs + express + pm2 + vuecli2",
    "author": "lihuyong <admin@lihuyong.com>",
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server  --inline --progress --config build/webpack.dev.conf.js",
        "build:test": "cross-env NODE_ENV=testing ENV_CONFIG=test node build/build.js",
        "start:test": "cross-env ENV_CONFIG=test pm2 start pm2.json",
        "restart:test": "cross-env ENV_CONFIG=test pm2 restart pm2.json",
        "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node build/build.js",
        "start": "cross-env ENV_CONFIG=prod pm2 start pm2.json",
        "restart": "cross-env ENV_CONFIG=prod pm2 restart pm2.json",
        "stop": "pm2 stop all",
        "delete": "pm2 delete all"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "body-parser": "^1.19.0",
        "cross-env": "^5.2.0",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "md5-node": "^1.0.1",
        "mysql": "^2.17.1",
        "pm2": "^3.5.1",
        "socket.io": "^2.3.0",
        "vue": "^2.5.2",
        "vue-router": "^3.0.1",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.22.1",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-plugin-transform-vue-jsx": "^3.5.0",
        "babel-preset-env": "^1.3.2",
        "babel-preset-stage-2": "^6.22.0",
        "chalk": "^2.4.2",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.0",
        "express": "^4.17.1",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^1.1.4",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "fs": "0.0.1-security",
        "html-webpack-plugin": "^2.30.1",
        "node-notifier": "^5.1.2",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "ora": "^1.2.0",
        "path": "^0.12.7",
        "portfinder": "^1.0.13",
        "postcss-import": "^11.0.0",
        "postcss-loader": "^2.0.8",
        "postcss-url": "^7.2.1",
        "rimraf": "^2.6.0",
        "semver": "^5.3.0",
        "shelljs": "^0.7.6",
        "uglifyjs-webpack-plugin": "^1.1.1",
        "url-loader": "^0.5.8",
        "vue-loader": "^13.3.0",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.5.2",
        "webpack": "^3.6.0",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-dev-server": "^2.9.1",
        "webpack-merge": "^4.1.0"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
